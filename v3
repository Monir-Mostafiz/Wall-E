void setup() {
  servor.attach(5);
  servol.attach(3);
  servo1.attach(6); 
  servo2.attach(7);
  servo3.attach(8);
  servo3.write(76);
  servol.write(lin);
  servor.write(rin);
  servo2.write(s2_in);
  Wire.begin();                                                          
  Serial.begin(9600);                                                     
  display.begin(SH1106_SWITCHCAPVCC);                                     
  display.display();                                                      
  delay(2000);                                                            
  display.clearDisplay();
  display.drawBitmap(0, 0, eye1, 128, 64, WHITE);
  display.display();
  pinMode(trigPin, OUTPUT);
  pinMode(echoPin, INPUT);
  pinMode(13,OUTPUT);
  pinMode(52,OUTPUT);
  servo4.attach(9);
  servo4.write(50);
 // delay(2000);
  //servo4.write(20);

    
}

void loop() {
  
         
  for (angle = startAngle; angle < endAngle; angle +=2) {
        int duration, cm;
  digitalWrite(trigPin, LOW);
  delayMicroseconds(2);
  digitalWrite(trigPin, HIGH);
  delayMicroseconds(10);
  digitalWrite(trigPin, LOW);
  duration = pulseIn(echoPin, HIGH);
  cm = (duration / 2) / 29.1; 
  Serial.print("Distance: ");
    Serial.print(cm);
    Serial.println(" cm");
  if (cm < 10 && cm > 0) { 

    //eye movement start
    delay(200);
    servo4.write(0);
    for(int i=s2_in; i>s2_end; i--)
     {
       servo2.write(i);
       delay(10);
     }
     digitalWrite(52,HIGH);
    //servo2.write(s2_end);
    randNumber = random(1000, 5000);
      display.drawBitmap(0, 0, eye1, 128, 64, WHITE);
      display.display(); 
    servol.write(lin);
    servor.write(rin);
    delay(2000);
    
    servor.write(rend);
    delay(1000);
      display.clearDisplay();                                               
      display.drawBitmap(0, 0, eye22, 128, 64, WHITE); 
      display.display();  
   servor.write(rin);
   delay(1000);
   digitalWrite(52,LOW);
    servor.write(rend);
    delay(150);
    servor.write(rin);
   servol.write(lin);
    delay(1500);

      //check for distance
    digitalWrite(trigPin, LOW);
  delayMicroseconds(2);
  digitalWrite(trigPin, HIGH);
  delayMicroseconds(10);
  digitalWrite(trigPin, LOW);
  duration = pulseIn(echoPin, HIGH);
  cm = (duration / 2) / 29.1; 
  Serial.print("Distance2: ");
    Serial.print(cm);
    Serial.println(" cm");
  if (cm > 10 && cm > 0) { 
   goto hm;
  }


    servol.write(lend);
    delay(1000);
      display.clearDisplay();
      display.drawBitmap(0, 0, eye1, 128, 64, WHITE);                 
      display.display(); 
      
    servol.write(lin);
    delay(1000);
    servol.write(lend);
    delay(150);

      //check for distance
    digitalWrite(trigPin, LOW);
  delayMicroseconds(2);
  digitalWrite(trigPin, HIGH);
  delayMicroseconds(10);
  digitalWrite(trigPin, LOW);
  duration = pulseIn(echoPin, HIGH);
  cm = (duration / 2) / 29.1; 
  Serial.print("Distance2: ");
    Serial.print(cm);
    Serial.println(" cm");
  if (cm > 10 && cm > 0) { 
   goto hm;
  }


    servol.write(lin);
        display.clearDisplay();
        display.drawBitmap(0, 0, eye3, 128, 64, WHITE);                 
        display.display(); 
    delay(1500);

      //check for distance
    digitalWrite(trigPin, LOW);
  delayMicroseconds(2);
  digitalWrite(trigPin, HIGH);
  delayMicroseconds(10);
  digitalWrite(trigPin, LOW);
  duration = pulseIn(echoPin, HIGH);
  cm = (duration / 2) / 29.1; 
  Serial.print("Distance2: ");
    Serial.print(cm);
    Serial.println(" cm");
  if (cm > 10 && cm > 0) { 
   goto hm;
  }


     display.clearDisplay();
      display.drawBitmap(0, 0, eye1, 128, 64, WHITE);                 
      display.display(); 
    servol.write(lin);
    servor.write(rin);
    delay(100);
    servol.write(30);
    servor.write(160);
    delay(150);
    for(int i=73; i>42; i--)
     {
       servo3.write(i);
       delay(10);
     }
    
   
   //check for distance
    digitalWrite(trigPin, LOW);
  delayMicroseconds(2);
  digitalWrite(trigPin, HIGH);
  delayMicroseconds(10);
  digitalWrite(trigPin, LOW);
  duration = pulseIn(echoPin, HIGH);
  cm = (duration / 2) / 29.1; 
  Serial.print("Distance2: ");
    Serial.print(cm);
    Serial.println(" cm");
  if (cm > 10 && cm > 0) { 
   goto hm;
  }
  delay(2000);
  for(int i=42; i<73; i++)
     {
       servo3.write(i);
       delay(10);
     }
    servol.write(lin);
    servor.write(rin);
    delay(500);
      display.clearDisplay();
      display.drawBitmap(0, 0, eye4, 128, 64, WHITE);                 
      display.display(); 
    display.clearDisplay();
  }
//eye movement end



  else
  {
    digitalWrite(13, LOW);
  }
  delay(50); 
  hm:
  servo2.write(s2_in);
  servo3.write(73);
  delay(100);
  servo1.write(angle);
  display.clearDisplay();
      display.drawBitmap(0, 0, eye1, 128, 64, WHITE);
      display.display(); 
  servo4.write(50);
  
  }










  for (angle = endAngle; angle >= startAngle; angle -= 2) {
        int duration, cm;
  digitalWrite(trigPin, LOW);
  delayMicroseconds(2);
  digitalWrite(trigPin, HIGH);
  delayMicroseconds(10);
  digitalWrite(trigPin, LOW);
  duration = pulseIn(echoPin, HIGH);
  cm = (duration / 2) / 29.1; 
  Serial.print("Distance: ");
    Serial.print(cm);
    Serial.println(" cm");
  if (cm < 10 && cm > 0) { 
    delay(200);
    servo4.write(0);
    for(int i=s2_in; i>s2_end; i--)
     {
       servo2.write(i);
       delay(10);
     }
     digitalWrite(52,HIGH);
    //servo2.write(s2_end);
    digitalWrite(13, HIGH);
    randNumber = random(1000, 5000);
    display.clearDisplay();
      display.drawBitmap(0, 0, eye1, 128, 64, WHITE);
      display.display(); 
    servol.write(lin);
    servor.write(rin);
    delay(2000);
    
    servor.write(rend);
    delay(1000);
      display.clearDisplay();                                               
      display.drawBitmap(0, 0, eye22, 128, 64, WHITE); 
      display.display();  
   servor.write(rin);
   digitalWrite(52,LOW);
   delay(1000);
    servor.write(rend);
    delay(150);
    servor.write(rin);
   servol.write(lin);
    delay(1500);
    digitalWrite(trigPin, LOW);
  delayMicroseconds(2);
  digitalWrite(trigPin, HIGH);
  delayMicroseconds(10);
  digitalWrite(trigPin, LOW);
  duration = pulseIn(echoPin, HIGH);
  cm = (duration / 2) / 29.1; 
  Serial.print("Distance2: ");
    Serial.print(cm);
    Serial.println(" cm");
  if (cm > 10 && cm > 0) { 
   goto hm2;
  }
    servol.write(lend);
    delay(1000);
      display.clearDisplay();
      display.drawBitmap(0, 0, eye1, 128, 64, WHITE);                 
      display.display(); 
      
    servol.write(lin);
    delay(1000);
    servol.write(lend);
    delay(150);
    digitalWrite(trigPin, LOW);
  delayMicroseconds(2);
  digitalWrite(trigPin, HIGH);
  delayMicroseconds(10);
  digitalWrite(trigPin, LOW);
  duration = pulseIn(echoPin, HIGH);
  cm = (duration / 2) / 29.1; 
  Serial.print("Distance2: ");
    Serial.print(cm);
    Serial.println(" cm");
  if (cm > 10 && cm > 0) { 
   goto hm2;
  }
    servol.write(lin);
        display.clearDisplay();
        display.drawBitmap(0, 0, eye3, 128, 64, WHITE);                 
        display.display(); 
    delay(1500);
    digitalWrite(trigPin, LOW);
  delayMicroseconds(2);
  digitalWrite(trigPin, HIGH);
  delayMicroseconds(10);
  digitalWrite(trigPin, LOW);
  duration = pulseIn(echoPin, HIGH);
  cm = (duration / 2) / 29.1; 
  Serial.print("Distance2: ");
    Serial.print(cm);
    Serial.println(" cm");
  if (cm > 10 && cm > 0) { 
   goto hm2;
  }
      display.clearDisplay();
      display.drawBitmap(0, 0, eye1, 128, 64, WHITE);                 
      display.display(); 
    servol.write(lin);
    servor.write(rin);
    delay(100);
    servol.write(30);
    servor.write(160);
    delay(150);
    for(int i=73; i>42; i--)
     {
       servo3.write(i);
       delay(10);
     }

    digitalWrite(trigPin, LOW);
  delayMicroseconds(2);
  digitalWrite(trigPin, HIGH);
  delayMicroseconds(10);
  digitalWrite(trigPin, LOW);
  duration = pulseIn(echoPin, HIGH);
  cm = (duration / 2) / 29.1; 
  Serial.print("Distance2: ");
    Serial.print(cm);
    Serial.println(" cm");
  if (cm > 10 && cm > 0) { 
   goto hm2;
  }
  delay(2000);
  for(int i=42; i<73; i++)
     {
       servo3.write(i);
       delay(10);
     }
     delay(100);
    servol.write(lin);
    servor.write(rin);
    delay(500);
      display.clearDisplay();
      display.drawBitmap(0, 0, eye4, 128, 64, WHITE);                 
      display.display(); 
    display.clearDisplay();
  }
  else
  {
    digitalWrite(13, LOW);
  }
  delay(50); 
  hm2:
  servo3.write(73);
  servo1.write(angle);
  delay(100);
  servo2.write(s2_in);
  display.clearDisplay();
      display.drawBitmap(0, 0, eye1, 128, 64, WHITE);
      display.display(); 
      servo4.write(50);
     }

  
}




